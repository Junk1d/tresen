{
    "expo": {
        "name": "tresen",
        "slug": "tresen",
        "version": "1.0.0",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#14151f"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "androidNavigationBar": {
            "visible": "immersive",
            "barStyle": "dark-content",
            "backgroundColor": "#00000080"
        },
        "androidStatusBar": {
            "hidden": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#14151f"
            },
            "package": "dev.junk1d.tresen"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-navigation-bar",
                {
                    "position": "relative",
                    "visibility": "hidden",
                    "behavior": "inset-swipe"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "7fa57e80-4915-4320-b94d-1f12f5601943"
            }
        }
    }
}
